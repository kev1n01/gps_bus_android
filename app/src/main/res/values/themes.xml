<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GPSUDH" parent="Theme.MaterialComponents.Light.NoActionBar">

        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryVariant">@color/black_300</item>
        <item name="colorOnPrimary">@color/yellow_50</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/green_200</item>
        <item name="colorSecondaryVariant">@color/green_600</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l" tools:ignore="ObsoleteSdkInt">?attr/colorPrimaryVariant</item>
    </style>
    
    <style name="AppThemeLauncher">
        <item name="android:windowBackground">@drawable/splash_screen</item>
    </style>
    </resources>