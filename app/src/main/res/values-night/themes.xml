<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GPSUDH" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryVariant">@color/black_300</item>
        <item name="colorOnPrimary">@color/yellow_50</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/black_300</item>
        <item name="colorSecondaryVariant">@color/black_100</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l" tools:ignore="ObsoleteSdkInt">?attr/colorPrimaryVariant</item>
    </style>
</resources>